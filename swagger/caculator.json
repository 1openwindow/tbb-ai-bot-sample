{
  "openapi": "3.0.0",
  "info": {
    "title": "Calculator API",
    "version": "1.0.0"
  },
  "paths": {
    "/caculator": {
      "get": {
        "summary": "Calculate the result of adding two numbers and multiplying the sum by 3",
        "operationId": "calculateNumbers",
        "parameters": [
          {
            "name": "firstNumber",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "secondNumber",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ]
      }
    }
  },
  "components": {
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-KEY"
      }
    }
  },
  "security": [
    {
      "ApiKeyAuth": []
    }
  ]
}